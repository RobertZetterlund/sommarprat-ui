// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Episode {
  dob        String?
  title      String
  date       String
  playlistId String  @id
  imageurl   String
  episodeUrl String
  yearAired  Int
}
